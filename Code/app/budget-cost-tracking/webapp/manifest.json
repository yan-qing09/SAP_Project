{
  "_version": "1.65.0",
  "sap.app": {
    "id": "budgetcosttracking",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.16.1",
      "toolsId": "2d70da0d-8563-43ea-a8b4-8308c6abf82d"
    },
    "dataSources": {
      "mainService": {
        "uri": "/odata/v4/project-budgeting/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "odataVersion": "4.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.131.1",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.ui.layout": {},  
        "sap.ui.table": {},
        "sap.suite.ui.microchart": {},
        "sap.viz": {},
        "sap.f": {},
        "sap.ui.integration": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "budgetcosttracking.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "controlAggregation": "pages",
        "controlId": "app",
        "transition": "slide",
        "type": "View",
        "viewType": "XML",
        "path": "budgetcosttracking.view"
      },
      "routes": [
        {
          "name": "RouteBudgetCostPage",
          "pattern": ":?query:",
          "target": [
            "TargetBudgetCostPage"
          ]
        },
        {
          "name": "RouteResourceAllocationPage",
          "pattern": "ResourceAllocation",
          "target": [
            "TargetResourceAllocationPage"
          ]
        },
        {
          "name": "RouteFinancialPerformanceReport",
          "pattern": "FinancialPerformanceReport",
          "target": [
            "TargetFinancialPerformanceReport"
          ]
        }
      ],
      "targets": {
        "TargetBudgetCostPage": {
          "id": "BudgetCostPage",
          "name": "BudgetCostPage"
        },
        "TargetResourceAllocationPage": {
          "id": "ResourceAllocationPage",
          "name": "ResourceAllocationPage"
        },
        "TargetFinancialPerformanceReport": {
            "id": "FinancialPerformanceReport",
            "name": "FinancialPerformanceReport"
        }
      }
    },
    "rootView": {
      "viewName": "budgetcosttracking.view.App",
      "type": "XML",
      "id": "App"
    }
  }
}
